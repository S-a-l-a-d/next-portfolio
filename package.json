{
  "name": "next-portfolio",
  "version": "1.0.0",
  "description": "My portfolio built with Next.js",
  "dependencies": {
    "async": "^2.6.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.2",
    "dotenv": "^6.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "isomorphic-unfetch": "^2.0.0",
    "lusca": "^1.6.0",
    "method-override": "^2.3.10",
    "mongoose": "^5.1.5",
    "next": "^6.0.3",
    "nodemailer": "^4.6.7",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "@types/async": "^2.0.49",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.36",
    "@types/dotenv": "^4.0.3",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.10",
    "@types/lusca": "^1.5.0",
    "@types/method-override": "^0.0.31",
    "@types/mongoose": "^5.0.16",
    "@types/next": "^6.0.1",
    "@types/node": "^10.3.4",
    "@types/nodemailer": "^4.6.1",
    "@types/react": "^16.4.0",
    "@types/react-dom": "^16.0.6",
    "@types/styled-jsx": "^2.2.3",
    "@zeit/next-typescript": "^1.1.0",
    "concurrently": "^3.5.1",
    "cross-env": "^5.2.0",
    "nodemon": "^1.17.5",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.9.2"
  },
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-next && npm run build-server && npm run tslint-next && npm run tslint-server",
    "serve": "cross-env NODE_ENV=production node .server/index.js",
    "watch": "concurrently -k \"tsc -w -p tsconfig.server.json\" \"nodemon .server/index.js\"",
    "build-next": "next build",
    "build-server": "tsc -p tsconfig.server.json",
    "tslint-next": "tslint -c tslint.json -p tsconfig.json",
    "tslint-server": "tslint -c tslint.json -p tsconfig.server.json",
    "tslint-next-fix": "tslint --fix -c tslint.json -p tsconfig.json",
    "tslint-server-fix": "tslint --fix -c tslint.json -p tsconfig.server.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect .server/index.js",
    "watch-debug": "concurrently -k \"tsc -w -p tsconfig.server.json\" \"nodemon --inspect .server/index.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/S-a-l-a-d/next-portfolio.git"
  },
  "author": "Salad <xa.lach@yahoo.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/S-a-l-a-d/next-portfolio/issues"
  },
  "homepage": "https://github.com/S-a-l-a-d/next-portfolio#readme"
}
